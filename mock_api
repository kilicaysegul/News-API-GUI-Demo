import time

def AA_API_Connect(zaman, progress_callback=None):
    """
    Dummy API simulation for demo purposes.
    Simulates downloading data and updates progress if a callback is provided.
    """
    total = 5
    for i in range(1, total + 1):
        time.sleep(0.5)  # simulate work
        if progress_callback:
            progress_callback(i, total)
    print(f"Dummy data fetched for time: {zaman}")
